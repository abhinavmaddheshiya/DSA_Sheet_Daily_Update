/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    bool hasCycle(ListNode *head) {
        if(head==NULL)return false;
        ListNode* temp=head;
        for(int i=0;i<=10000;i++){
            temp=temp->next;
            if(temp==NULL)return false;
        }
        return true;
    }
};
